{
  "name": "gatsby-plugin-starter",
  "version": "0.1.0",
  "description": "A simple starter to get up and developing Gatsby plugins quickly.",
  "author": "Alex Moon <alex.jared.moon@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "starter",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby/tree/master/starters/plugin"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prebuild": "npm run lint",
    "lint": "eslint ./src",
    "prepack": "cross-env NODE_ENV=production npm run build",
    "test": "npm run build && jest --coverage"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.1.4",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.16.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  }
}
